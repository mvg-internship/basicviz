<logic_scheme>
  <elements>
    <!--Every element has id, x and y coordinates of it's top-left point and height and width-->
    <element e_id="0" x="0.0" y="0.0" height="0.4" width="0.4">
      <!--Element may have connections-->
      <!--Every connection has id and id of element it goes into-->
      <connection c_id="0" end_element="2">
        <!--Connection has to have start and end points and may have another vertices. It is obligatory to declare start_point as the first child and end_point as the last child-->
        <!--Start and end points along with vertices have x and y coordinates as their attributes-->
        <start_point x="0.4" y="0.2"/>
        <vertex x="0.5" y="0.3"/>
        <end_point x="0.6" y="0.2"/>
      </connection>
      <connection c_id="1" end_element="3">
        <start_point x="0.2" y="0.4"/>
        <vertex x="0.25" y="0.5"/>
        <vertex x="0.35" y="0.55"/>
        <end_point x="0.2" y="0.6"/>
      </connection>
    </element>
    <element e_id="1" x="0.6" y="0.0" height="0.4" width="0.4">
      <connection c_id="2" end_element="4">
        <start_point x="0.8" y="0.4"/>
        <end_point x="0.8" y="0.6"/>
      </connection>
    </element>
    <element e_id="2" x="0.0" y="0.6" height="0.4" width="0.4">
      <connection c_id="3" end_element="4">
        <start_point x="0.4" y="0.8"/>
        <end_point x="0.6" y="0.8"/>
      </connection>
    </element>
    <element e_id="3" x="0.6" y="0.6" height="0.4" width="0.4">
      <!--More connections...-->
    </element>
    <!--More elements...-->
  </elements>
</logic_scheme>

